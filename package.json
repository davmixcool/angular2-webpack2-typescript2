{
  "name": "angular2-webpack",
  "version": "0.0.7",
  "description": "Angular2, Webpack2, Material Design, Typescript2, Docker, Sass, Codelyzer",
  "license": "MIT",
  "main": "",
  "engines": {
    "node": ">= 4.4.7",
    "npm": ">= 3"
  },
  "scripts": {
    "initialize": "docker-compose build",
    "clean": "rimraf dist/frontend",
    "tslint": "tslint ./src/frontend/app/**/*.ts",
    "serve": "webpack-dev-server --config webpack/webpack.dev.js --inline --progress --profile --watch --content-base src/frontend --host=0.0.0.0",
    "build": "rimraf dist/frontend && webpack --config webpack/webpack.prod.js  --progress --profile  --bail",
    "serve:dist": "http-server dist/frontend  -p 8080 ",
    "docker:install": "docker-compose run frontend npm install",
    "docker:serve:dist": "docker-compose run --service-ports frontend npm run serve-dist",
    "docker:build": "docker-compose run frontend npm run build",
    "docker:serve": "docker-compose run --service-ports frontend npm run serve",
    "docker:tslint": "docker-compose run --service-ports frontend npm run tslint",
    "docker:bash": "docker-compose run --service-ports frontend bash",
    "ngc": "./node_modules/.bin/ngc"
  },
